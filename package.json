{
  "name": "reducer-generator-object-map",
  "version": "1.1.2",
  "description": "A redux reducer-generator which builds a redux-reducer which matches against object literals.",
  "main": "dist/reducer-generator-object-map.js",
  "author": "Braden R. Napier",
  "license": "MIT",
  "scripts": {
    "test": "cross-env BABEL_ENV=commonjs NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js",
